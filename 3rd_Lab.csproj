        c6.RemoveAll(e => (e == 0));
        
        
        len_c6 = c6.Count;
        for (int i = 0; i < len_c6; i = i + 1)
        {
            Console.Write("{0:f} ", c6[i]);
        }
        Console.WriteLine("");
    }
